                        <tr>
                            <td>
                                <table id="otrack" align="center" cellspacing="0" cellpadding="0" border="0" width="630">
                                    <tr>
                                        <td height="1" width="7"><img alt="" border="0" height="1" width="7" style="display: none;" src="http://kompakt.handelsblatt-service.com/ff/ch.php?cmd=op114155766"></td>
                                    </tr>
</table>
                                <table align="center" cellspacing="0" cellpadding="0" border="0" width="630">
                                    <tr>
                                        <td valign="top" width="630"><img src="http://kompakt.handelsblatt-service.com/res/381/2012/bsc/hdr_2012.png" border="0" hspace="0" vspace="0" alt="" style="display:block;" width="630" height="111"></td>
</tr>
